{
  "name": "koishi-plugin-nhentai-downloader",
  "description": "nhentai 漫画插件，提供搜索、下载、打包发送漫画作品的功能，支持 PDF/ZIP 加密",
  "version": "1.3.7",
  "author": "YuzuharaYuka",
  "homepage": "https://github.com/YuzuharaYuka/koishi-plugin-nhentai-downloader",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuzuharaYuka/koishi-plugin-nhentai-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/YuzuharaYuka/koishi-plugin-nhentai-downloader/issues"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "wasm-dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run build:wasm && npm run build:ts",
    "build:ts": "tsc",
    "build:wasm": "cd wasm-image-processor && cross-env RUSTFLAGS=\"-C target-feature=+simd128 --cfg getrandom_backend=\\\"wasm_js\\\"\" wasm-pack build --target nodejs --release --out-dir ../wasm-dist && node ../scripts/post-wasm-build.js",
    "build:wasm:unix": "cd wasm-image-processor && sh build.sh",
    "build:wasm:win": "cd wasm-image-processor && build.bat",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "nhentai",
    "downloader",
    "manga",
    "comic",
    "search"
  ],
  "peerDependencies": {
    "koishi": "^4.18.0"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.81",
    "archiver": "^7.0.1",
    "archiver-zip-encrypted": "^2.0.0",
    "got-scraping": "^4.1.2",
    "pdfkit": "^0.17.2"
  },
  "devDependencies": {
    "@types/pdfkit": "^0.17.3"
  }
}
